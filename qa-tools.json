{
	"composerBinDir": "vendor\/bin",
	"projectName": "StepupGateway",
	"buildArtifacts": {
		"enabled": false
	},
	"travis": {
		"enabled": true,
		"phpVersions": {
			"1": "5.6",
			"2": "5.5",
			"3": "5.4"
		},
		"env": {
			"enabled": true,
			"vars": [
				"SYMFONY_ENV=test"
			]
		},
		"slack": {
			"enabled": false
		}
	},
	"enablePhpTools": true,
	"enableComposer": false,
	"enablePhpLint": true,
	"enablePhpMessDetector": true,
	"phpMdExcludePatterns": [
		"*\/Tests\/*"
	],
	"enablePhpCodeSniffer": true,
	"phpCodeSnifferCodingStyle": "PSR2",
	"phpCsExcludeSymfony": true,
	"phpCsExcludeCustomPatterns": [
		"*\/Tests\/*"
	],
	"phpCsExcludePatterns": [
		"src\/*\/*Bundle\/Resources",
		"src\/*\/*Bundle\/Tests",
		"src\/*\/Bundle\/*Bundle\/Resources",
		"src\/*\/Bundle\/*Bundle\/Tests",
		"*\/Tests\/*"
	],
	"enablePhpCopyPasteDetection": true,
	"phpCpdExcludePatterns": [
		"*\/Tests\/*"
	],
	"enablePhpSecurityChecker": false,
	"phpSrcPath": [
		"src"
	],
	"enablePhpUnit": true,
	"customPhpUnitXml": true,
	"phpUnitConfigPath": "app\/phpunit.xml.dist",
	"enableJsTools": false,
	"enableJsHint": false,
	"enableBehat": false,
	"enablePreCommitHook": true,
	"md5Command": "md5sum",
	"_qa_tools_run_completed": true
}